<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <description>输出验证码</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>PictureCheckCode</servlet-name>
    <servlet-class>rj7.servlet.PictureCheckCode</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PictureCheckCode</servlet-name>
    <url-pattern>/PictureCheckCode</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>发送邮件</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>MailServ</servlet-name>
    <servlet-class>rj7.servlet.MailServ</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MailServ</servlet-name>
    <url-pattern>/MailServ</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>找回密码</description>
