    <servlet-name>FindPswdServ</servlet-name>
    <servlet-class>rj7.servlet.FindPswdServ</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FindPswdServ</servlet-name>
    <url-pattern>/FindPswdServ</url-pattern>
  </servlet-mapping>
  
 <servlet>
